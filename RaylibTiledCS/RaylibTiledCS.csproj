<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <PropertyGroup>
      <Version>0.0.1</Version>
      <Authors>Zapturk</Authors>
      <Company>Zapturk</Company>
      <PackageId>RaylibTiledCS</PackageId>
      <Description>
        RaylibTiledCS is a library for loading Tiled maps and tilesets with Raylib-CS. This version supports Tiled 1.10.2 and Raylib-CS v6.0.0 
      </Description>
      <RepositoryUrl>https://github.com/zapturk/RaylibTiledCS.git</RepositoryUrl>
      <PackageTags>tiled,tmx,tsx,raylib</PackageTags>
      <GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>

</Project>
